{
  "name": "negotiate-ai",
  "private": true,
  "type": "module",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --build --force",
    "lint": "eslint . --fix",
    "install-husky": "cd .. && husky install frontend/.husky",
    "prepare": "cd .. && husky frontend/.husky",
    "lint-staged": "lint-staged",
    "format": "prettier --write '**/*.{js,ts,vue,html,css}'",
    "generate-types": "pocketbase-typegen --db ../orchestration/pocketbase/pb_data/data.db --out src/composables/pocketbase/types.ts",
    "generate-client": "openapi --input http://127.0.0.1:8000/openapi.json --output ./src/services --client axios --name ApiClient"
  },
  "dependencies": {
    "@fontsource/roboto": "5.2.5",
    "@mdi/font": "7.4.47",
    "@tanstack/vue-query": "^5.76.0",
    "axios": "^1.9.0",
    "dayjs": "^1.11.13",
    "echarts": "^5.6.0",
    "pinia": "^3.0.2",
    "pocketbase": "^0.26.0",
    "v-plausible": "^1.2.0",
    "validator": "^13.15.15",
    "vue": "^3.5.13",
    "vue-echarts": "^7.0.3",
    "vue-router": "^4.5.1",
    "vuetify": "^3.8.3"
  },
  "lint-staged": {
    "**/*.{js,ts,vue,html,css}": "prettier --write",
    "**/*.{js,ts,vue}": "eslint --fix"
  },
  "devDependencies": {
    "@tsconfig/node22": "^22.0.1",
    "@types/node": "^22.15.12",
    "@types/validator": "^13.15.2",
    "@vitejs/plugin-vue": "^5.2.3",
    "@vue/tsconfig": "^0.7.0",
    "eslint": "^9.26.0",
    "eslint-config-vuetify": "^4.0.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.5.1",
    "npm-run-all2": "^8.0.1",
    "openapi-typescript-codegen": "^0.29.0",
    "pocketbase-typegen": "^1.3.1",
    "prettier": "^3.5.2",
    "sass-embedded": "^1.87.0",
    "typescript": "~5.8.3",
    "unplugin-auto-import": "^19.1.2",
    "unplugin-fonts": "^1.3.1",
    "unplugin-vue-components": "^28.5.0",
    "unplugin-vue-router": "^0.12.0",
    "vite": "^6.3.5",
    "vite-plugin-vuetify": "^2.1.1",
    "vue-tsc": "^2.2.10"
  }
}
